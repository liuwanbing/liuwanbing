	<Fish kind="39" name="鱼王10" speed="5" multiple="0" BoundingBox="150,150" hitRadius="10" captureProbability="0.1" />

	<!-- kind:对应BulletKind name:描述 speed:子弹速度 netRadius:渔网的半径 -->
	<Bullet kind="0" name="1炮筒" speed="20" netRadius="70" />
